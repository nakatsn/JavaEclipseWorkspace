package nntest.setvarience;

import java.text.SimpleDateFormat;
import java.util.Calendar;
import java.util.HashSet;
import java.util.Set;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Main main = new Main();
		
		Set<Integer> s1 = new HashSet<Integer>();
		Set<Integer> s2 = new HashSet<Integer>();
		
		main.log("start");
		main.constructSetData(s1);
		main.constructSetData(s2);
		main.log("end");
	}
	
	private void constructSetData(Set<Integer> s) {
		int add = 0;
		int count = 0;
		for (int i = 0; i < 10000; i++) {
			add = (int)Math.random() * 10;
			count += add;
			s.add(new Integer(count));
		}
	}
	
	private void log(String s) {
		Calendar c = Calendar.getInstance();
		SimpleDateFormat sdf = new SimpleDateFormat("yyyy/MM/dd HH:mm:ss.SSS : ");
		
		System.out.println(sdf.format(c.getTime()) + s);
	}

}
